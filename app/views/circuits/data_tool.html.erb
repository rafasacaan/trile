<%=render "graphs_data_tool"%>

<div class="panel panel-default tab-box">
    <div class="panel-heading">
      <h4><i class="fa fa-angle-right"></i>Detalle del Circuito</h4>
    </div>
        <section id="no-more-tables">  
            <table class="table table-striped table-advance cf" id="circuit-table">
               <thead class="cf">
                      <tr>
                          <th class="numeric">Select</th>
                          <th>ID</th>
                          <th>Description</th>
                          <th class="numeric">Tipe</th>
                          <th class="numeric">Status</th>                  
                      </tr>
                </thead>
                <% @circuits.each do |circuit|%>
                <tbody>
                   <tr>
                      <td data-title="Selected">
                        <span class="check">
                          <input type="checkbox" class="checked" id="<%= circuit.description.to_sym %>">
                        </span>
                      </td>
                      <td data-title="ID"><%= circuit.id %></td>
                      <td data-title="Descripcion"><%= circuit.description %></td>
                      <td class="numeric" data-title="Tipo"><%= circuit.type %></td>
                      <td class="numeric" data-title="Estado">
                        <span class="label label-<%=circuit_status(circuit.status)%> label-mini">
                          <%= circuit.status %>
                        </span></td>           
                   </tr>          
                </tbody>                  
                <% end %>       
            </table>
        </section>
</div>
<!--/showback -->
<div class="row">
  <div class="col-md-1 col-md-offset-11">
    <%= link_to "< Circuitos", circuits_path, type: "button", class: "btn btn-success btn-xs" %>          
  </div>
</div>