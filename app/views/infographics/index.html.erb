
<div class="row">
<div class="col-xs-6 col-md-6">
	<div id="donut-example"></div>
<% @circuits.each do |c| %>
<div class="row">
  <div class="col-xs-1 col-md-1"><%= c.description %></div>
  <div class="col-xs-9 col-md-10">
  	<div class="progress progress-striped active">
    <div class="progress-bar progress-bar-<%=cycle("success","info","warning","danger")%>" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 45%">
   <span class="sr-only">45% Complete</span>
   </div>
 	</div>
  </div>
  <div class="col-xs-2 col-md-1"><%= number_to_currency(c.measures.count) %></div>
</div>
<%end%>
</div> 
<div class="col-xs-6 col-md-6"></div>

</div>
<!-- ANIMATED PROGRESS BARS -->
<%= @data.inspect %>

	


